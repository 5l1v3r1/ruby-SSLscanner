#!/usr/bin/env ruby
# 
# ScanSSL gem.
# Some info here.
#

require 'optparse'

unless File.respond_to? :realpath
  class File #:nodoc:
    def self.realpath path
      return realpath(File.readlink(path)) if symlink?(path)
      path
    end
  end
end
$: << File.expand_path(File.dirname(File.realpath(__FILE__)) + '/../lib')

require 'scanssl'
require 'rubygems'

puts "#{ScanSSL::VERSION}"
